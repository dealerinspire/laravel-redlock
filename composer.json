{
    "name": "dealerinspire/laravel-redlock",
    "description": "Redis distributed locks for laravel",
    "license": "MIT",
    "keywords": ["redlock", "laravel redis lock", "redis lock"],
    "authors": [
      {
        "name": "LibiChai",
        "email": "chaiguoxing@qq.com"
      },
      {
        "name": "Daniel Kuck-Alvarez",
        "email": "dankuck@gmail.com"
      },
      {
        "name": "Potsky",
        "email": "potsky@me.com"
      }
    ],
    "require": {
      "php": ">=8.0.0",
      "illuminate/support": ">=10.0",
      "illuminate/console": ">=10.0",
      "predis/predis": "^2.0"
    },
    "require-dev": {
      "orchestra/testbench": "8.*|9.*",
      "php-mock/php-mock-mockery": "^1.1",
      "phpunit/phpunit": "~9.0|^10.5"
    },
    "autoload": {
      "psr-4": {
        "DealerInspire\\RedLock\\": "src/"
      }
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
      ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "DealerInspire\\RedLock\\RedLockServiceProvider"
        ]
      }
    },
    "scripts": {
      "test": [
        "phpunit"
      ]
    },
    "minimum-stability": "stable"
  }
  
